    <h2 class="danger text-center">Productos cargados</h2>
    <% if(listaProductos.length > 0){ %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr class="warning">
                <th>Nombre</th>
                <th>Precio</th>
                <th>Imagen</th>
            </tr>
            <% for (const producto of listaProductos) { %>
            <tr>
                <td><%=producto.title %></td>             
                <td><%=producto.price %></td>             
                <td><img style="max-width: 50px;" src="<%=producto.thumbnail %>" alt=""></td>             
            </tr>
            <% }; %>
        </table>
    </div>
    <% } else { %>
    <h3 class="alert alert-danger text-center">No hay productos</h3>
    <% } %>
    <div style="text-align: center;margin-top:2em">
        <a href='http://localhost:8080/'>Formulario</a>
    </div>